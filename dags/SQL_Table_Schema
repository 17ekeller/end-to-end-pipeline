CREATE TABLE Weather_Data(
    name VARCHAR(255),
    region VARCHAR(255),
    temp_f DOUBLE,
    condition_text VARCHAR(255),
    wind_mph DOUBLE,
    wind_dir VARCHAR(255),
    precip_in DOUBLE,
    humidity INT,
    cloud INT,
    feelslike_f DOUBLE,
    vis_miles DOUBLE,
    uv DOUBLE,
    gust_mph DOUBLE,
    local_time TIMESTAMP,
)

